or=\"(item, index) in sliderUrls\"\n          :key=\"index\"\n          :src=\"item.url\"\n          alt=\"Слайдер №\"\n        />\n        <button\n          class=\"next\"\n          @click=\"\n            forward();\n            makeSoundSlider();\n          \"\n        >\n          &gt;\n        </button>\n        <button\n          class=\"prev\"\n          @click=\"\n            back();\n            makeSoundSlider();\n          \"\n        >\n          &lt;\n        </button>\n      </div>\n    </div>\n    <div class=\"iconsBox\">\n      <div class=\"icons\">\n        <!-- Рисуем иконки и присваиваем класс checked по номеру активного слайда. -->\n        <div\n          class=\"icon\"\n          :class=\"{\n            checked: activeSlide === index + countToShow,\n          }\"\n          v-for=\"(item, index) in iconsQuantityArr\"\n          :key=\"index.id\"\n        ></div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { mapGetters, mapMutations, mapActions } from 'vuex'\nexport default {\n  name: 'ProjectDetails',\n  data () {\n    return {\n      activeSlide: 0,\n      countToShow: 0, // нужен для изменения кол-ва кругов показа\n      iconsQuantityArr: [1] * 3 // массив для отражаемых иконок\n    }\n  },\n  created () {\n    this.SET_SliderImagesUrls(this.fetchDataSliderImagesUrls())\n    this.SET_Visibility(true) //  передаем true во флаг страницы NotFound\n  },\n  methods: {\n    makeSound () {\n      const audio = new Audio(\n        require('../../../assets/sounds/knopka-v-prostranstve-priglushennyii-blizkii.mp3')\n      )\n      audio.play()\n    },\n    makeSoundSlider () {\n      const audio = new Audio(\n        require('../../../assets/sounds/jeleznaya-knopka-vyiklyucheniya1.mp3')\n      )\n      audio.play()\n    },\n    // регистрируем все геттеры\n    ...mapGetters([\n      'getTitleProjectDetais',\n      'getDescriptonProjectDetais',\n      'getSliderImagesUrls'\n    ]),\n    ...mapMutations([\n      'SET_SliderImagesUrls',\n      'ADD_SliderList',\n      'SET_Visibility'\n    ]),\n    ...mapActions(['fetchDataSliderImagesUrls']),\n\n    // если выходим за границы слайдера то будем повторять показ, увеличивая массив адресов слайдерских картинок\n    getImageClass (index) {\n      if (index < this.activeSlide) {\n        return 'outLeft'\n      }\n      if (index === this.activeSlide) {\n        return 'active'\n      }\n      if (index > this.activeSlide) {\n        return 'outRight'\n      }\n    },\n\n    // если выходим за границы слайдера то будем повторять показ, увеличивая наш массив адресов слайдерских картинок\n    forward () {\n      if (this.activeSlide < this.sliderUrls.length - 1) {\n        this.activeSlide++\n      } else {\n        if (this.activeSlide === this.sliderUrls.length - 1) {\n          // если он будет на последнем слайде, то увеличим массив слайдера на такой же массив и пойдем по нему снова показывать все\n          this.ADD_SliderList()\n          setTimeout(() => {\n            // нужна небольшая задержка на переходе с поcледнего слайда для плавности\n            this.activeSlide++\n            if (this.countToShow === 0) {\n              this.countToShow += 3\n            } else {\n              this.countToShow = this.countToShow + 3\n            }\n          }, 10)\n        }\n      }\n    },\n    back () {\n      if (this.activeSlide > 0) {\n        this.activeSlide--\n      }\n    }\n  },\n  computed: {\n    title () {\n      return this.$store.getters.getTitleProjectDetails\n    },\n    description () {\n      return this.$store.getters.getDescriptonProjectDetails\n    },\n    sliderUrls () {\n      return this.$store.getters.getSliderImagesUrls\n    }\n  }\n}\n\n// слайдер\n</script>\n\n<style lang=\"scss\" scoped>\n.banner {\n  height: 350px;\n  background: url(\"../../../../public/projectDetailsImages/Banner.svg\")\n    no-repeat;\n  background-position: 50% 50%;\n  margin-bottom: 150px;\n}\n\n.informBox {\n  display: flex;\n  justify-conten