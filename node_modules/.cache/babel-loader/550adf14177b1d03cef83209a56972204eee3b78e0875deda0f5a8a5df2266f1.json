лище\n      if (localStorage.getItem('starKey')) {\n        const data = localStorage.getItem('starKey').split(',')\n        if (data.includes((this.item.id).toString())) {\n          this.classStar = 'selected star'\n          return 'selected star'\n        } else {\n          this.classStar = 'star'\n          return 'star'\n        }\n      } else {\n        // если данных нет в хранилище\n        this.classStar = 'star'\n        return 'star'\n      }\n    }\n  }\n\n}\n</script>\n\n<style lang=\"scss\" scoped></style>\n"],"mappings":";AA2FA;EACAA,IAAA;EACAC,KAAA;EACAC,KAAA;IACA;MACAC,SAAA;MACAC,UAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAD,UAAA;EACA;EACAE,OAAA;IACAC,UAAA;MACA,MAAAC,KAAA,OAAAC,KAAA,CACAC,OAAA,wEACA;MACAF,KAAA,CAAAG,IAAA;IACA;IACAC,kBAAA;MACA,MAAAJ,KAAA,OAAAC,KAAA,CACAC,OAAA,wEACA;MACAF,KAAA,CAAAG,IAAA;IACA;IACAE,WAAA;MACA,KAAAN,SAAA;MACA;MACA,SAAAJ,SAAA;QACA,KAAAA,SAAA;QACA;QACA,KAAAW,YAAA,CAAAC,OAAA;UACA,MAAAC,UAAA,QAAAC,IAAA,CAAAC,EAAA;UACAJ,YAAA,CAAAK,OAAA,YAAAH,UAAA;QACA;UACA;UACA,MAAAd,IAAA,GAAAY,YAAA,CAAAC,OAAA,YAAAK,KAAA;UACAlB,IAAA,CAAAmB,IAAA,MAAAJ,IAAA,CAAAC,EAAA;UACA,MAAAI,MAAA,OAAAC,GAAA,CAAArB,IAAA;UACA,MAAAsB,OAAA,GAAAC,KAAA,CAAAC,IAAA,CAAAJ,MAAA;UACA,MAAAN,UAAA,GAAAQ,OAAA,CAAAG,IAAA;UACAb,YAAA,CAAAK,OAAA,YAAAH,UAAA;QACA;MACA;QACA,KAAAb,SAAA;QACA;QACA,MAAAD,IAAA,GAAAY,YAAA,CAAAC,OAAA,YAAAK,KAAA;QACA,MAAAQ,KAAA,GAAA1B,IAAA,CAAA2B,OAAA,MAAAZ,IAAA,CAAAC,EAAA,CAAAY,QAAA;QACA5B,IAAA,CAAA6B,MAAA,CAAAH,KAAA;QACA,MAAAZ,UAAA,GAAAd,IAAA,CAAAyB,IAAA;QACAb,YAAA,CAAAK,OAAA,YAAAH,UAAA;MACA;IACA;IACAgB,YAAA;MACA;MACA,IAAAlB,YAAA,CAAAC,OAAA;QACA,MAAAb,IAAA,GAAAY,YAAA,CAAAC,OAAA,YAAAK,KAAA;QACA,IAAAlB,IAAA,CAAA+B,QAAA,MAAAhB,IAAA,CAAAC,EAAA,CAAAY,QAAA;UACA,KAAA3B,SAAA;UACA;QACA;UACA,KAAAA,SAAA;UACA;QACA;MACA;QACA;QACA,KAAAA,SAAA;QACA;MACA;IACA;EACA;AAEA"},"metadata":{},"sourceType":"module","externalDependencies":[]}